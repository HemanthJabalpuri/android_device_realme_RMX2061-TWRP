# mount point    fstype   device                  device2                flags
/boot             emmc    /dev/block/bootdevice/by-name/boot
/recovery         emmc    /dev/block/bootdevice/by-name/recovery         flags=backup
/cache            ext4    /dev/block/bootdevice/by-name/cache
/persist          ext4    /dev/block/bootdevice/by-name/persist
/dtbo             emmc    /dev/block/bootdevice/by-name/dtbo             flags=backup
/vbmeta           emmc    /dev/block/bootdevice/by-name/vbmeta           flags=backup;flashimg
/vbmeta_system    emmc    /dev/block/bootdevice/by-name/vbmeta_system
/vbmeta_vendor    emmc    /dev/block/bootdevice/by-name/vbmeta_vendor

/firmware         vfat    /dev/block/bootdevice/by-name/modem            flags=mounttodecrypt;fsflags="ro,shortname=lower,uid=1000,gid=1000,dmask=227,fmask=337,context=u:object_r:firmware_file:s0"
/modem            emmc    /dev/block/bootdevice/by-name/modem            flags=backup
/bluetooth        emmc    /dev/block/bootdevice/by-name/bluetooth        flags=backup;subpartitionof=/modem
/dsp              emmc    /dev/block/bootdevice/by-name/dsp              flags=backup;subpartitionof=/modem

# EFS partitions
/efs1             emmc    /dev/block/bootdevice/by-name/modemst1         flags=backup
/efs2             emmc    /dev/block/bootdevice/by-name/modemst2         flags=backup;subpartitionof=/efs1
/efsc             emmc    /dev/block/bootdevice/by-name/fsc              flags=backup;subpartitionof=/efs1
/efsg             emmc    /dev/block/bootdevice/by-name/fsg              flags=backup;subpartitionof=/efs1

# Removable storage
/external_sd      auto    /dev/block/mmcblk1p1    /dev/block/mmcblk1
/usb-otg          auto    /dev/block/sda1         /dev/block/sda         flags=storage;removable;wipeingui
